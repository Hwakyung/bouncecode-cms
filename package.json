{
  "name": "bouncecode-cms",
  "version": "0.1.0",
  "description": "바운스코드 콘텐츠 관리 시스템입니다.",
  "main": "index.js",
  "engines": {
    "node": "10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "next": "next",
    "dev": "NODE_ENV=development nodemon --watch server --exec 'ts-node' --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./server/index.ts",
    "start": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" ./server/index.ts",
    "certconfig": "openssl ecparam -genkey -name secp256r1 -out .private.certconfig.js && openssl ec -in .private.certconfig.js -pubout -out .public.certconfig.js",
    "postcertconfig": "> certconfig.js && echo 'module.exports={PRIVATE:`' >> certconfig.js && cat .private.certconfig.js >> certconfig.js && echo '`,PUBLIC:`' >> certconfig.js && cat .public.certconfig.js >> certconfig.js && echo '`}' >> certconfig.js && rm .private.certconfig.js .public.certconfig.js",
    "lint": "tslint --project tsconfig.json",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && npm install && npm run dev"
  },
  "author": "BounceCode, Inc.",
  "license": "AGPL-3.0",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.9",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^2.13.0",
    "core-js": "2",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.2.6",
    "graphql-type-json": "^0.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "next": "^9.3.6",
    "nodemon": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^8.10.1",
    "type-graphql": "^1.0.0-rc.1",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "devDependencies": {}
}
